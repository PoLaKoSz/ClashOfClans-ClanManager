{
    "name": "polakosz/clash-of-clans-clanmanager",
    "type": "library",
    "description": "The best tool to save time when needs to manage one or more ClashOfClans Clan.",
    "keywords": [
        "polakosz",
        "clash-of-clans-clanmanager",
        "PoLáKoSz",
        "CoC",
        "Clash of Clans",
        "Clan",
        "Clan Manager",
        "ClanManager"
    ],
    "homepage": "https://github.com/PoLaKoSz/ClashOfClans-ClanManager",
    "license": "MPL",
    "authors": [{
        "name": "Tom PoLáKoSz",
        "email": "polakosz@freemail.hu",
        "homepage": "https://polakosz.000webhostapp.com/",
        "role": "Developer"
    }],
    "require": {
        "illuminate/support": "~5.1",
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "PoLaKoSz\\CoC_ClanManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "polakosz\\CoC_ClanManager\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "config": {
        "sort-packages": true
    }
}